#include<stdio.h>
#include<stdlib.h>
int main()
{ printf("这是一个判断某一年是否为闰年的程序\n请输入年份，并按回车键\n");
  int year;
  scanf("%d",&year);
  if (year%4!=0)
  {printf("%d年不是闰年\n",year);
  }
  else if(year%400==0)
  {printf("%d年是闰年\n",year);
  }
  else if(year%100==0)
  {printf("%d年不是闰年\n",year);
  }
  else
  {printf("%d年是闰年\n",year);
  }
  system("pause");
  return 0;
}
